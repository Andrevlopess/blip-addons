{
  "name": "Blips",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Collection of Blip utilities",
  "homepage_url": "https://duo.com/labs",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_title": "TSRWPCX",
    "default_popup": "popup.html"
  },
  "default_locale": "en",
  "background": {
    "scripts": ["js/background.js"],
    "persistent": false
  },
  "permissions": ["https://*/*", "webNavigation"],
  "content_security_policy": "default-src 'self';",
  "web_accessible_resources": ["js/listener.js"],
  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": ["*://*.blip.ai/*"],
      "js": ["js/content.js"]
    }
  ]
}
